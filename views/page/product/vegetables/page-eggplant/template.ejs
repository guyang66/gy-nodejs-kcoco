          <%- include('../../../../component/apply/layout_tabs');%>
        </div>
        <div class="tabs-content-wrap" data-anchor="case">
          <%- include('../../../../component/case/layout-6');%>
        </div>
        <div class="tabs-content-wrap" data-anchor="history">
          <%- include('../../../../component/history/layout-slide');%>
        </div>
