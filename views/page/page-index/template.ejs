<!DOCTYPE html>
<html class="no-js" lang="zh">
  <head>
    <meta charset="utf-8">
    <%- include('../../layout/commonHead')%>
    <!--预加载-->
    <link rel="preload" href="/images/logo/white.svg" as="image">
    <link rel="preload" href="/images/logo/blue.svg" as="image">
    <link rel="preload" href="/images/phone/phone-blue.svg" as="image">
    <link rel="preload" href="/images/phone/phone-white.svg" as="image">

    <!--css-->
    <link rel="stylesheet" href="/css/page-index/style.css">
    <!--css 也有插入顺序的，不然会发生覆盖的情况，注意优先级-->
    <link rel="stylesheet" href="/css/variables.css">
    <link rel="stylesheet" href="/css/component.css">
    <link rel="stylesheet" href="/css/common.css">
    <link rel="stylesheet" href="/css/layout.css">
    <link rel="stylesheet" href="/css/global.css">
    <link rel="stylesheet" href="<%=getContext('swiper.css','css')%>">
    <link rel="stylesheet" href="/common/plugins/video-js/video-js.css">

    <!--js-->
    <script src="<%=getContext('jquery.min.js','commonjs')%>"></script>
    <script src="<%=getContext('swiper.js','commonjs')%>"></script>
    <script src="<%=getContext('/countup/jquery.countup.min.js','commonjs')%>"></script>
    <script src="<%=getContext('/countup/jquery.waypoints.min.js','commonjs')%>"></script>

  </head>
  <body>
    <div class="main-layout">
      <%- include('../../layout/header');%>
      <section class="main-wrap">
        <div class="main-content">
          <section class="home-wrap">
            <section class="index-page-wrap">
              <div class="index-content">
                <%- include('./module-banner');%>
                <%- include('./module-product');%>
                <%- include('./module-solution');%>
                <%- include('./module-customer');%>
                <%- include('./module-logo');%>
                <%- include('./module-statics');%>
                <%- include('./module-certify');%>
                <%- include('./module-resource');%>
                <%- include('./module-news');%>
                <%- include('../../component/form');%>
              </div>
            </section>
          </section>
        </div>
      </section>

      <%- include('../../layout/footer');%>
      <%- include('../../component/sideMenus');%>

    </div>
    <script src="<%=getContext('common.js')%>"></script>
    <script src="<%=getContext('index/index.js')%>"></script>
    <%- include('../../layout/commonFoot');%>
  </body>
</html>

